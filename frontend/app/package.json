{
  "name": "rocket-meals-dev",
  "main": "expo-router/entry",
  "homepage": "/swosy",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "predeploy": "expo export -p web",
    "deploy": "npm run copy-assets && gh-pages --nojekyll -d dist",
    "copy-assets": "cpx \"node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/**/*\" \"dist/assets/node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts\"",
    "deploy:gh:pages": "yarn run predeploy && yarn run deploy",
    "deploy:local": "npm run export:web:dev && mkdir -p ./serveDist/rocket-meals && cp -r ./dist/* ./serveDist/rocket-meals/ && npx serve serveDist",
    "export:web": "expo export --platform web",
    "export:web:dev": "expo export --platform web"
  },
  "jest": {
    "preset": "jest-expo"
  },
